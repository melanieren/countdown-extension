{
  "manifest_version": 2,

  "name": "Countdown",
  "description": "This extension keeps track of the time left until an event.",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "storage",
    "https://ajax.googleapis.com/"
  ]
}
